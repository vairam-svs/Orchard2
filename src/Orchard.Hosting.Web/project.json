{
  "version": "2.0.0-*",
  "dependencies": {
    "Microsoft.AspNetCore.Diagnostics": "1.0.0",
    "Microsoft.AspNetCore.Http": "1.0.0",
    "Microsoft.AspNetCore.Mvc": "1.0.0",
    "Microsoft.AspNetCore.Routing": "1.0.0",
    "Microsoft.AspNetCore.StaticFiles": "1.0.0",
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.0.0",
    "Microsoft.Extensions.Logging": "1.0.0",
    "Orchard.Abstractions": "2.0.0-*",
    "Orchard.ContentManagement": "2.0.0-*", // Only to discover services automatically
    "Orchard.ContentManagement.Display": "2.0.0-*", // Such that Razor Views can reference the Tag Helpers
    "Orchard.DisplayManagement": "2.0.0-*",
    "Orchard.Environment.Commands": "2.0.0-*",
    "Orchard.Environment.Extensions": "2.0.0-*",
    "Orchard.Environment.Navigation": "2.0.0-*",
    "Orchard.Events": "2.0.0-*",
    "Orchard.Hosting": "2.0.0-*",
    "Orchard.DeferredTasks": "2.0.0-*",
    "Orchard.Validation": "2.0.0-*",
    "Orchard.Data": "2.0.0-*",
    "Orchard.Recipes.Abstractions": "2.0.0-*", // Such that Razor Views can resolve references
    "Orchard.ResourceManagement": "1.0.0-*",
    "Orchard.BackgroundTasks": "1.0.0-*"
  },
  "buildOptions": {
    "define": [ "TRACE" ],
    "debugType": "portable",
    "warningsAsErrors": true
  },
  "frameworks": {
    "netstandard1.6": {
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ],
      "dependencies": {
        "NETStandard.Library": "1.6.0-*",
        "System.Diagnostics.Process": "4.1.0-*"
      }
    }
  },
  "packOptions": {
    "type": "git",
    "url": "https://github.com/OrchardCMS/Orchard2"
  }
}