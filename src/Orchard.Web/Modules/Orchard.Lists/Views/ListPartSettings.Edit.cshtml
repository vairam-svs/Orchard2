@model Orchard.Lists.Models.ListPartSettings

<fieldset class="form-group">
    <div class="row col-xs-4">
        <label asp-for="ContainedContentType">@T["Contained Content Type"]</label>
        <input asp-for="ContainedContentType" type="text" class="form-control">
        <span class="hint">@T["The content type that this list can contain, e.g. BlogPost for a Blog."]</span>
    </div>
</fieldset>